    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="sayi, i, j, bayrak" type="Integer" array="False" size=""/>
            <assign variable="bayrak" expression="0"/>
            <output expression="&quot;2 Tam Say&#305;n&#305;n Karesi &#350;eklinde Yaz&#305;l&#305;p Yaz&#305;lmad&#305;&#287;&#305;n&#305; Merak Etti&#287;iniz Say&#305;y&#305; Giriniz!&quot;" newline="True"/>
