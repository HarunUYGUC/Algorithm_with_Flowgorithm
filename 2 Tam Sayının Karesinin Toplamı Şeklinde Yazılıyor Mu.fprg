                        <else/>
                    </if>
                </for>
            </for>
            <if expression="bayrak == 0">
                <then>
                    <output expression="&quot;2 Tam Say&#305;n&#305;n Karesi &#350;eklinde Yaz&#305;lam&#305;yor!&quot;" newline="True"/>
                </then>
                <else/>
