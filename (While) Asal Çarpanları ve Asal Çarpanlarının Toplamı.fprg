    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="sayi, bolen, bolenToplam&#305;" type="Integer" array="False" size=""/>
            <assign variable="bolenToplam&#305;" expression="0"/>
            <output expression="&quot;Asal &#199;arpanlar&#305;n&#305; Bulmak &#304;stedi&#287;iniz Say&#305;y&#305; Giriniz!!&quot;" newline="True"/>
            <input variable="sayi"/>
            <assign variable="bolen" expression="2"/>
            <while expression="sayi &gt; 1">
                <if expression="sayi % bolen == 0">
                    <then>
                        <output expression="bolen" newline="True"/>
                        <assign variable="sayi" expression="sayi / bolen"/>
                        <assign variable="bolenToplam&#305;" expression="bolenToplam&#305; + bolen"/>
                    </then>
                    <else>
                        <assign variable="bolen" expression="bolen + 1"/>
                    </else>
                </if>
            </while>
            <output expression="&quot;Asal &#199;arpanlar&#305;n&#305;n Toplam&#305;= &quot; &amp; bolenToplam&#305;" newline="True"/>
        </body>
    </function>
